{
    "name": "rmcgirr83/hidemyprofile",
    "type": "phpbb-extension",
    "description": "This Extension will allow your members to choose to hide their profile or not.  Permission based allowing friends to view your profile. Admins and mods are always allowed to view others profiles.",
    "version": "1.0.6",
    "time": "2024-07-17",
    "keywords": [
        "phpbb",
        "extension",
        "hide my profile",
        "communication"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Rich McGirr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phpbb/phpbb": ">=3.3",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Hide My Profile",
        "soft-require": {
            "phpbb/phpbb": ">=3.3"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/hide_my_profile",
            "filename": "version_check",
            "ssl": true
        }
    }
}